            "request": "launch",
            "build": {
                "docker": {
                    "path": "Dockerfile"
                }
            },
            "image": "Rhyquiem",
            "service": {
                "name": "rhyquiem",
                "containerPort": 8080,
                "resources": {
                    "limits": {
                        "cpu": 1,
                        "memory": "256Mi"
                    }
                }
            },
            "target": {
                "minikube": {}
